<h1>Hello world!</h1>
{{#each entry}}
<div class="twelve columns">
    <div class="two columns">
        <img src={{thumbnail}} alt="reddit-icon" class="thumbnail">
    </div>
    <div class="ten columns">
        <p class='headline'>{{Title}}</h1>
        <p class="author">Submitted by: {{author}}</p>
        <a href={{link}}>Link</a>
    </div>
</div>
{{/each}}